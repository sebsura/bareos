Job {
  Name = "ai-1"
  JobDefs = "DefaultJob"
  Client = "bareos-fd"
  FileSet = "SelfTest"
  Accurate = yes
  AlwaysIncremental = yes
  AlwaysIncrementalJobRetention = 1 seconds
  AlwaysIncrementalKeepNumber = 1
  AlwaysIncrementalMaxFullAge = 1 seconds
  Priority = 10
  Allow Mixed Priority = no
  Spool Data = no
  Run Script {
      Runs When = Before
      Command = "sleep 5"
      Command = "/home/ssura/update-test-data.sh"
  }
}

